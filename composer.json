{
    "name": "waygou/surveyor",
    "description": "Surveyor - Manage your global scopes and policies in a Laravel application.",
    "type": "library",
    "keywords": [
        "waygou",
        "xheetah",
        "laravel",
        "php"
    ],
    "require": {
        "php": "^7.1",
        "waygou/helpers": "dev-master",
        "illuminate/support": "5.6.*|5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Waygou\\Surveyor\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waygou\\Surveyor\\ServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Waygou",
            "email": "bruno.falcao@waygou.com"
        }
    ],
    "minimum-stability": "dev"
}
